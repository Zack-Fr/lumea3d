{
  "name": "@lumea/shared",
  "version": "0.0.1",
  "description": "Shared types, schemas, and utilities for Lumea",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.esm.json && tsc -p tsconfig.build.cjs.json && node scripts/rename-cjs.mjs",
    "dev": "tsc -p tsconfig.build.esm.json --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo 'No tests implemented yet' && exit 0"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.1.6"
  }
}