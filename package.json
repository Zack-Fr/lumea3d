{
  "name": "lumea",
  "version": "0.0.1",
  "description": "Lumea - AI-powered interior layout generator with explainable spatial reasoning",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "test:cov": "pnpm --filter @lumea/api test:cov",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "generate:openapi": "node scripts/generate-openapi.mjs",
    "generate:client": "pnpm generate:openapi && pnpm generate:client:types && pnpm generate:client:sdk",
    "generate:client:types": "openapi-typescript ./openapi.json -o ./packages/shared/src/types/api.ts",
    "generate:client:sdk": "openapi-generator-cli generate -i ./openapi.json -g typescript-axios -o ./packages/api-client/src/generated --additional-properties=typescriptThreePlus=true,withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "artillery": "^2.0.24",
    "autocannon": "^8.0.0",
    "axios": "^1.11.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express-prom-bundle": "^8.0.0",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "prom-client": "^15.1.3",
    "socket.io-client": "^4.7.2",
    "typescript": "^5.9.2",
    "winston": "^3.17.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0",
      "tmp": ">=0.2.4"
    }
  }
}